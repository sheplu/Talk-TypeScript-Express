{
  "name": "demo-ts-node",
  "version": "1.0.0",
  "description": "demo TypeScript / Node",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Jean Burellier",
  "license": "ISC",
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
